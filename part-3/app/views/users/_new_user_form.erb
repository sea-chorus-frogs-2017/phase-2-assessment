
<div class = "new-user-holder">
  <form id="new-user-form" method="post" action="/users">
  <fieldset>
    <label for ="username">Username</label>
    <input id = "username" type="text" class="form-control" name="user[username]" ">
    <br>
    <label for ="email">Email</label>
    <input id = "email" type="email" class="form-control" name="user[email]" >
    <br>
    <label for ="password">Password</label>
    <input id = "password" type="password" class="form-control" name="user[password]">
    <br>
    <input type="submit" class="btn btn-default" value="create">
  </fieldset>
  </form>
</div>
